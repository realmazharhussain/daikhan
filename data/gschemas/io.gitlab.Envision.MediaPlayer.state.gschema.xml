<schemalist gettext-domain="gdm-settings">
  <schema id="io.gitlab.Envision.MediaPlayer.state"
       path="/io/gitlab/Envision/MediaPlayer/state/">

     <key name="width" type="u">
       <default>601</default>
     </key>

     <key name="height" type="u">
       <default>430</default>
     </key>

     <key name="maximized" type="b">
       <default>false</default>
     </key>

     <key name="queue" type="as">
       <default>[]</default>
     </key>

     <key name="track" type="i">
       <range min="-1"/>
       <default>-1</default>
     </key>

     <key name="paused" type="b">
       <default>true</default>
     </key>

     <key name="volume" type="d">
       <default>0.5</default>
       <range min="0" max="2"/>
     </key>

  </schema>
</schemalist>
